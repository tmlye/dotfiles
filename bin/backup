#!/bin/bash
# Folders
rsync -a --delete $HOME/.aws $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/code $HOME/mount/backup
rsync -a --delete $HOME/desktop $HOME/mount/backup
rsync -a --delete $HOME/downloads $HOME/mount/backup
rsync -a --delete $HOME/.config $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.dotfiles $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.electrum $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.gem $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.gnupg $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.gtktheme $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.irssi $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.lastpass $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.mozilla $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.mpd $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.mpdscribble $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.ssh $HOME/mount/backup/OS/home/
rsync -a --delete $HOME/.wallpaper $HOME/mount/backup/OS/home/
rsync -a --delete /etc/openvpn $HOME/mount/backup/OS/
# Files
rsync -a $HOME/.asoundrc $HOME/mount/backup/OS/home/
rsync -a $HOME/.gtkrc-2.0 $HOME/mount/backup/OS/home/
rsync -a $HOME/.gitconfig $HOME/mount/backup/OS/home/
rsync -a $HOME/.mpdconf $HOME/mount/backup/OS/home/
rsync -a /etc/default/grub $HOME/mount/backup/OS/

# vim:set filetype=sh:
