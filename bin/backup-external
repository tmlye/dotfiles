#!/bin/bash
export AWS_PROFILE=personal-backup
duplicity --exclude-filelist $HOME/mount/backup/duplicity-exclude-list \
          --allow-source-mismatch \
          --verbosity 8 \
          --tempdir . \
          --num-retries 5 \
          --asynchronous-upload \
          --volsize 100 \
          --s3-use-ia \
          $HOME/mount/ boto3+s3://duplicity-backup-j7v3a
unset AWS_PROFILE
# vim:set filetype=sh:
